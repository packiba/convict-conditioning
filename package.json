{
  "name": "convict-conditioning",
  "version": "1.0.0",
  "description": "web app for working out",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/packiba/convict-conditioning.git"
  },
  "keywords": [
    "mern",
    "react"
  ],
  "author": "Pavel Kiba <packiba@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/packiba/convict-conditioning/issues"
  },
  "homepage": "https://github.com/packiba/convict-conditioning#readme",
  "dependencies": {
    "config": "^3.3.3",
    "express": "^4.17.1",
    "express-validator": "^6.8.0",
    "mongoose": "^5.11.6"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6"
  }
}
